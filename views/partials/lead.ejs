<div id="lead">
    <div id="lead-content">
        <% if(typeof thanks !== 'undefined' && thanks == true) { %>
            <h1>Thank You</h1>
            <h2>
                <%= name %>
            </h2>
            <h3>Your message has been sent successfully. I will be in touch with you soon.</h3>
            <a href="/images/Preston's_Resume.pdf" class="btn-rounded-white">Download Resume</a>
            <% }else { %>
                <h1>Preston Hill</h1>
                <h2>Software Developer</h2>
                <a href="/images/Preston's_Resume.pdf" target="_blank" class="btn-rounded-white">Download Resume</a>
                <%  } %>


                    <!-- End #lead-content -->
    </div>
    <div id="lead-overlay"></div>

    <div id="lead-down">
        <span>
            <i class="fa fa-chevron-down" aria-hidden="true"></i>
        </span>
    </div>
    <!-- End #lead-down -->
</div>